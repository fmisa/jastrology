     description="Jastrology Readme 文档">

     <createForPack name="Base" />
   </shortcut>

   <shortcut
     name="Jastrology Uninstaller"
     programGroup="yes"
     desktop="no"
     applications="no"
     startMenu="no"
     startup="no"
     target="$INSTALL_PATH\Uninstaller\uninstaller.jar"
     commandLine=""
	 iconFile="%SystemRoot%\system32\SHELL32.dll"
     iconIndex="31"
     description="This uninstalls Jastrology">

     <createForPack name="Base" />
