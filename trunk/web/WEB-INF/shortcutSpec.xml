<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>

  <skipIfNotSupported />

  <programGroup defaultName="星天下\Jastrology" location="applications"/>


  <shortcut
     name="Jastrology"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
     target="$INSTALL_PATH\jastrology.bat"
     workingDirectory="$INSTALL_PATH"     
     commandLine=""
     description="启动 Jastrology"
     iconFile="$INSTALL_PATH\star.ico"
